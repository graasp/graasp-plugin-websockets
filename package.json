    "fastify-plugin": "^3.0.0",
    "fastify-websocket": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-plugin-jest": "^24.3.2",
    "jest": "^26.6.3",
