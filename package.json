{
  "name": "graasp-websockets",
  "version": "1.0.0",
  "description": "Websockets extension for graasp",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "yarn build",
    "prepack": "yarn build",
    "clean": "npx shx rm -rf dist/ coverage/",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "yarn build && npx jest",
    "test:ci": "npx jest --ci --colors",
    "prettier:check": "prettier --check \"{src,test}/**/*.ts\"",
    "prettier:write": "prettier --write \"{src,test}/**/*.ts\"",
    "pre-commit": "yarn prettier:check && yarn lint",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "post-commit": "git status"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/graasp/graasp-websockets.git"
  },
  "keywords": [
    "graasp",
    "websockets"
  ],
  "author": "React EPFL",
  "contributors": [
    "Alexandre Chau",
    "Andr√© Nogueira"
  ],
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/graasp/graasp-websockets/issues"
  },
  "homepage": "https://github.com/graasp/graasp-websockets#readme",
  "dependencies": {
    "ajv": "^8.6.2",
    "dotenv": "^9.0.2",
    "fastify": "^3.18.1",
    "fastify-plugin": "^3.0.0",
    "fastify-websocket": "^3.2.0",
    "ioredis": "^4.27.6"
  },
  "devDependencies": {
    "@commitlint/cli": "16.0.1",
    "@commitlint/config-conventional": "16.0.0",
    "@types/graasp": "github:graasp/graasp-types",
    "@types/ioredis": "^4.26.4",
    "@types/jest": "^26.0.23",
    "@types/ws": "^7.4.5",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "7.0.4",
    "jest": "^26.6.3",
    "prettier": "^2.5.1",
    "shx": "^0.3.3",
    "ts-jest": "^26.5.6",
    "typescript": "^4.3.4",
    "wait-for-expect": "^3.0.2"
  },
  "packageManager": "yarn@3.2.0-rc.3"
}
