{
  "name": "@graasp/plugin-websockets",
  "version": "1.0.0",
  "description": "Websockets extension for graasp",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "yarn build",
    "prepack": "yarn build",
    "clean": "npx shx rm -rf dist/ coverage/",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "yarn build && npx jest",
    "test:ci": "npx jest --ci --colors",
    "prettier:check": "prettier --check \"{src,test}/**/*.ts\"",
    "prettier:write": "prettier --write \"{src,test}/**/*.ts\"",
    "pre-commit": "yarn prettier:check && yarn lint",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "post-commit": "git status"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/graasp/graasp-plugin-websockets.git"
  },
  "keywords": [
    "graasp",
    "websockets"
  ],
  "author": "React EPFL",
  "contributors": [
    "Alexandre Chau",
    "Andr√© Nogueira"
  ],
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/graasp/graasp-plugin-websockets/issues"
  },
  "homepage": "https://github.com/graasp/graasp-plugin-websockets#readme",
  "dependencies": {
    "@fastify/websocket": "5.0.1",
    "ajv": "8.12.0",
    "dotenv": "16.0.3",
    "fastify": "3.29.5",
    "fastify-plugin": "3.0.1",
    "ioredis": "5.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@graasp/sdk": "github:graasp/graasp-sdk#97-add-websocket-types",
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/ioredis": "5.0.0",
    "@types/jest": "29.4.0",
    "@types/ws": "8.5.4",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "prettier": "2.8.4",
    "shx": "0.3.4",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5",
    "wait-for-expect": "3.0.2"
  },
  "packageManager": "yarn@3.2.1"
}
